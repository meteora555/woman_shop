{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts","**/*.js", "**/*.tsx","**/*.tsx", "pages/api/_document.js", "components/index.js", "pages/catalog/catalog.tsx", "components/little/NavBar.jsx", "components/large/HeaderCatalog.tsx", "components/large/ModalCart.tsx", "components/little/CatalogItem.tsx"],
  "exclude": ["node_modules"]
}
